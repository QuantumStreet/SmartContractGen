{
  "license": "MIT",
  "pragmaVersion": "^0.8.0",
  "contractType": "Contract",
  "name": "Counter",
  "state": [
    {
      "name": "count",
      "type": "uint256",
      "visibility": "private",
      "description": "Current count value"
    }
  ],
  "functions": [
    {
      "name": "increment",
      "visibility": "public",
      "description": "Increments the counter",
      "body": [
        "count += 1;"
      ]
    },
    {
      "name": "getCount",
      "visibility": "public",
      "description": "Returns the current count",
      "returns": "uint256",
      "modifiers": [
        "view"
      ],
      "body": [
        "return count;"
      ]
    }
  ]
}
